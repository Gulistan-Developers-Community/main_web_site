{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "exclude": [
    "node_modules",
    ".yarn",
    "post.config.js",
    "next.config.js",
    "tailwind.config.js"
  ],
  "include": [
    "next-env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/components/Header.js",
    "src/components/Header.js",
    "src/components/Logo.js",
    "src/components/Layout.js",
    "src/components/Layout.js",
    "src/components/seo.js",
    "src/components/seo.js",
    "src/pages/cabinet/editing.js"
  ]
}
